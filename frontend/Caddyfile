# global options
{
	admin off
	persist_config off
	auto_https off
	log {
		format json
	}
	servers {
		trusted_proxies static private_ranges 100.0.0.0/8
	}
}

:{$PORT:3001} {
	log {
		format json
	}

	rewrite /health /index.html
	root * dist
	encode gzip
	file_server
	try_files {path} /index.html
}
